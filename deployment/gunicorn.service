[Unit]
Description=gunicorn daemon for Hewor Project
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/hewor_project
ExecStart=/home/ubuntu/hewor_project/.venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/hewor_project/hewor_project.sock hewor_project.wsgi:application

[Install]
WantedBy=multi-user.target
